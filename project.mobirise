              "position": "relative",
              "height": "100%",
              "display": "flex",
              "flex-flow": "column nowrap",
              "justify-content": "flex-start",
              "padding": "24px",
              "border-radius": "1rem",
              "@media (min-width: 768px)": {
                "padding": "27px"
              },
              "@media (min-width: 992px)": {
                "padding": "50px"
              }
            },
            ".item-content": {
              "width": "100%"
            },
            ".label-text": {
              "color": "#ffffff",
              "opacity": "0.8",
              "width": "100%",
              "margin-bottom": "16px"
            },
            ".mbr-text": {
              "color": "#ffffff",
              "margin-top": "12px",
              "margin-bottom": "0",
              "opacity": "0.8"
            },
            ".item-title": {
              "color": "#ffffff",
              "margin-top": "12px",
              "margin-bottom": "0"
            },
            ".list-container": {
              "display": "flex",
              "flex-direction": "column",
              "margin-top": "18px"
            },
            ".list-item": {
              "&:not(:last-child)": {
                "margin-bottom": "16px"
              },
              "display": "flex",
              "align-items": "center"
            },
            ".icon-box": {
              "padding-right": "14px"
            },
            ".list-text": {
              "flex-grow": "1",
              "color": "#ffffff",
              "margin-bottom": "0"
            },
            ".link-container": {
              "display": "flex",
              "flex-direction": "column",
              "margin-top": "50px",
              "@media (min-width: 992px)": {
                "margin-top": "72px"
              },
              ".link": {
                "&:first-child": {
                  "margin-bottom": "16px"
                },
                "span": {
                  "padding-right": "10px"
                }
              }
            }
