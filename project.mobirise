                  "&:first-child": {
                    "padding-right": "0.5rem"
                  },
                  "&:last-child": {
                    "padding-left": "0.5rem"
                  }
                }
              },
              "& when (@showContentBg)": {
                "&:first-child": {
                  ".item-wrap": {
                    "background": "@contentColor1",
                    "& when (@linerGradient1)": {
                      "background": "linear-gradient(180deg, @contentColor1, @gradientColor1)",
                      "& when (@horizontallyGradient1)": {
                        "background": "linear-gradient(90deg, @contentColor1, @gradientColor1) !important"
                      }
                    }
                  }
                },
                "&:last-child": {
                  ".item-wrap": {
                    "background": "@contentColor2",
                    "& when (@linerGradient2)": {
                      "background": "linear-gradient(180deg, @contentColor2, @gradientColor2)",
                      "& when (@horizontallyGradient2)": {
                        "background": "linear-gradient(90deg, @contentColor2, @gradientColor2) !important"
                      }
                    }
                  }
                }
              }
            },
