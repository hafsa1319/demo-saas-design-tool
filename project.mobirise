entColor2\" value=\"#0099ff\" condition=\"linerGradient2 && showContentBg && cardAmount == '2'\">\n        <input type=\"checkbox\" title=\"Horizontally\" name=\"horizontallyGradient2\" condition=\"showContentBg && linerGradient2 && cardAmount == '2'\">\n\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" required>\n            <input type=\"image\" title=\"Image\" value=\"@THEME_PATH@/components/_images/background2.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#242424\" selected>\n        </fieldset>\n        <input type=\"checkbox\" title=\"Liner Gradient\" name=\"linerGradient\" condition=\"bg.type == 'color'\">\n        <input type=\"checkbox\" title=\"Horizontally\" name=\"horizontallyGradient\" condition=\"bg.type == 'color' && linerGradient\">\n        <input type=\"color\" title=\"Gradient Color\" name=\"gradientColor\" value=\"#000000\" condition=\"linerGradient && bg.type == 'color'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\" checked>\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#1a1a1a\" condition=\"overlay && bg.type == 'image'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.7\" condition=\"overlay && bg.type == 'image'\">\n    </mbr-parameters>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\"></div>\n    <div class=\"container\">\n        <div class=\"row\">\n            <div class=\"col-12 col-items\">\n                <div class=\"item сol-12 col-lg-6\">\n                    <div class=\"item-wrap\">\n                        <div class=\"item-content\">\n                            <p class=\"label-text mbr-fonts-style\" mbr-if=\"showCardLabel\" mbr-theme-style=\"display-4\" data-app-selector=\".label-text\">\n                                React Components\n                            </p>\n 